<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.skillsexchange.model">

	<class name="User" table="users">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		
		<property name="first_name" column="first_name" />
		<property name="last_name" column="last_name" />
		<property name="sex" column="sex" />
		<property name="birth" column="birth" />
		<property name="country" column="country" />
		<property name="city" column="city" />
		<property name="summary" column="summary" />
		
		
	</class>

</hibernate-mapping>